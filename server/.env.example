# AI Provider Configuration
# Choose one: 'gemini' or 'github' or 'openai'
AI_PROVIDER=gemini

# Gemini Configuration (using OpenAI SDK with custom base URL)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
GEMINI_CHAT_MODEL=gemini-2.0-flash-exp
GEMINI_EMBEDDING_MODEL=text-embedding-004

# GitHub Models Configuration (using OpenAI SDK with custom base URL)
GITHUB_TOKEN=your_github_token_here
GITHUB_BASE_URL=https://models.github.ai/inference
GITHUB_CHAT_MODEL=openai/gpt-4o-mini
GITHUB_EMBEDDING_MODEL=text-embedding-3-small

# OpenAI Configuration (fallback/original)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_CHAT_MODEL=gpt-4
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=langchainjs-testing

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379